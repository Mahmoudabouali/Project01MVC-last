@using Mvc.PresentationLayer.ViewModels
@model UserViewModel

<div class="form-group">
    <label asp-for="Id"></label>
    <input readonly class="form-control" asp-for="Id" />
</div>
<div class="form-group">
    <label asp-for="FristName"></label>
    <input class="form-control" asp-for="FristName" />
    <span class="text-danger" asp-validation-for="FristName"></span>
</div>
<div class="form-group">
    <label asp-for="LastName"></label>
    <input class="form-control" asp-for="LastName" />
    <span class="text-danger" asp-validation-for="LastName"></span>
</div>
<div class="form-group">
    <label asp-for="UserName"></label>
    <input readonly class="form-control" asp-for="UserName" />
</div>
<div class="form-group">
    <label asp-for="Email"></label>
    <input readonly class="form-control" asp-for="Email" />
</div>

